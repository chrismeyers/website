{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "22"
  },
  "type": "module",
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage --run",
    "test:ci": "CI=true vitest --coverage --run",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --experimental-cli --check '**/*.{ts,tsx,js,cjs,jsx}'",
    "format:fix": "prettier --experimental-cli --write '**/*.{ts,tsx,js,cjs,jsx}'",
    "dev": "vite",
    "preview": "vite preview",
    "generate": "bin/generate.sh",
    "build": "tsc --build && vite build"
  },
  "dependencies": {
    "lightgallery": "^2.9.0",
    "lodash": "^4.17.21",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-switch": "^7.1.0",
    "wouter": "^3.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.18.8",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.11",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^3.2.4"
  }
}
