{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "20"
  },
  "type": "module",
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage --run",
    "test:ci": "CI=true vitest --coverage --run",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check '**/*.{ts,tsx,js,cjs,jsx}'",
    "format:fix": "prettier --write '**/*.{ts,tsx,js,cjs,jsx}'",
    "dev": "vite",
    "preview": "vite preview",
    "generate": "bin/generate.sh",
    "build": "tsc && vite build"
  },
  "dependencies": {
    "lightgallery": "^2.7.2",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "react-switch": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.17.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.4",
    "eslint": "^9.14.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.11.0",
    "jsdom": "^25.0.1",
    "prettier": "3.3.3",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.12.2",
    "vite": "^5.4.10",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.4"
  }
}
