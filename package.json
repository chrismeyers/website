{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "24"
  },
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ci": "CI=true vitest run --coverage",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --experimental-cli --check '**/*.{ts,tsx,js,cjs,jsx}'",
    "format:fix": "prettier --experimental-cli --write '**/*.{ts,tsx,js,cjs,jsx}'",
    "dev": "vite",
    "preview": "vite preview",
    "generate": "bin/generate.sh",
    "build": "tsc --build && vite build"
  },
  "dependencies": {
    "lightgallery": "^2.9.0",
    "lodash": "^4.17.23",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-switch": "^7.1.0",
    "wouter": "^3.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/lodash": "^4.17.23",
    "@types/node": "^24.10.10",
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/coverage-v8": "^4.0.18",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "jsdom": "^28.0.0",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^4.0.18"
  }
}
