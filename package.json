{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "vite",
    "generate": "bin/generate.sh",
    "build": "rimraf ./build && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage --run",
    "test:ci": "CI=true vitest --coverage --run",
    "lint": "eslint . --ext .js,.mjs,.jsx --max-warnings 0",
    "lint:fix": "eslint . --fix  --ext .js,.mjs,.jsx",
    "format": "prettier --write '**/*.{js,mjs,jsx}'",
    "format:check": "prettier --check '**/*.{js,mjs,jsx}'"
  },
  "dependencies": {
    "lightgallery": "^2.7.2",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "react-switch": "^7.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/eslint": "^8.44.9",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^23.0.1",
    "prettier": "3.1.1",
    "rimraf": "^5.0.5",
    "vite": "^5.0.12",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.0.4"
  }
}
