{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "22"
  },
  "type": "module",
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage --run",
    "test:ci": "CI=true vitest --coverage --run",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check '**/*.{ts,tsx,js,cjs,jsx}'",
    "format:fix": "prettier --write '**/*.{ts,tsx,js,cjs,jsx}'",
    "dev": "vite",
    "preview": "vite preview",
    "generate": "bin/generate.sh",
    "build": "tsc && vite build"
  },
  "dependencies": {
    "lightgallery": "^2.8.3",
    "lodash": "^4.17.21",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-switch": "^7.1.0",
    "wouter": "^3.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.9",
    "eslint": "^9.23.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "prettier": "3.5.3",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.3",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.0.9"
  }
}
