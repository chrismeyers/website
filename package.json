{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "20"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "generate": "bin/generate.sh",
    "build": "rimraf ./build && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage --run",
    "test:ci": "CI=true vitest --coverage --run",
    "lint": "eslint . --ext .js,.mjs,.jsx --max-warnings 0",
    "lint:fix": "eslint . --fix  --ext .js,.mjs,.jsx",
    "format": "prettier --write '**/*.{js,mjs,jsx}'",
    "format:check": "prettier --check '**/*.{js,mjs,jsx}'"
  },
  "dependencies": {
    "lightgallery": "^2.7.2",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0",
    "react-switch": "^7.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.4",
    "@types/eslint": "^8.56.10",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.5.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.0.0",
    "prettier": "3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.5.1"
  }
}
