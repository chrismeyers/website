{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "20"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "generate": "bin/generate.sh",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage --run",
    "test:ci": "CI=true vitest --coverage --run",
    "lint": "eslint . --ext .ts,.tsx,.js,.cjs,.jsx --max-warnings 0",
    "lint:fix": "eslint . --fix  --ext .ts,.tsx,.js,.cjs,.jsx",
    "format": "prettier --check '**/*.{ts,tsx,js,cjs,jsx}'",
    "format:fix": "prettier --write '**/*.{ts,tsx,js,cjs,jsx}'"
  },
  "dependencies": {
    "lightgallery": "^2.7.2",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "react-switch": "^7.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/eslint": "^8.56.10",
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jsdom": "^25.0.0",
    "prettier": "3.3.3",
    "typescript": "5.5.4",
    "vite": "^5.4.3",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^2.0.5"
  }
}
