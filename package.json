{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "18"
  },
  "scripts": {
    "dev": "vite",
    "generate": "node bin/node/resume-parser.mjs resume/LaTeX/Meyers_Chris/Meyers_Chris_Resume.tex src/assets/generated/resume.js",
    "build": "rimraf ./build && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage --run",
    "test:ci": "CI=true vitest --coverage --run",
    "lint": "eslint . --ext .js,.mjs,.jsx --max-warnings 0",
    "lint:fix": "eslint . --fix  --ext .js,.mjs,.jsx"
  },
  "dependencies": {
    "lightgallery": "^2.7.2",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-switch": "^7.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.5",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.1.0",
    "prettier": "3.0.3",
    "rimraf": "^5.0.1",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^4.0.0",
    "vitest": "^0.34.5"
  }
}
