{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "22"
  },
  "type": "module",
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage --run",
    "test:ci": "CI=true vitest --coverage --run",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check '**/*.{ts,tsx,js,cjs,jsx}'",
    "format:fix": "prettier --write '**/*.{ts,tsx,js,cjs,jsx}'",
    "dev": "vite",
    "preview": "vite preview",
    "generate": "bin/generate.sh",
    "build": "tsc && vite build"
  },
  "dependencies": {
    "lightgallery": "^2.7.2",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "wouter": "^3.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.16.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.13.0",
    "jsdom": "^25.0.1",
    "prettier": "3.4.2",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.0.3",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.8"
  }
}
