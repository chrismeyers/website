{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "generate": "node bin/node/resume-parser.mjs resume/LaTeX/Meyers_Chris/Meyers_Chris_Resume.tex src/assets/generated/resume.js",
    "build": "rimraf ./build && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage --run",
    "test:ci": "CI=true vitest --coverage --run",
    "lint": "eslint . --ext .js,.mjs,.jsx --max-warnings 0",
    "lint:fix": "eslint . --fix  --ext .js,.mjs,.jsx"
  },
  "dependencies": {
    "lightgallery": "^2.7.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "react-switch": "^7.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^21.1.1",
    "prettier": "2.8.7",
    "rimraf": "^5.0.0",
    "vite": "^4.2.2",
    "vite-plugin-svgr": "^2.4.0",
    "vitest": "^0.30.1"
  }
